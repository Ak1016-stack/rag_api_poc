root ::= tool_call | final

tool_call ::= "{" ws "\"type\"" ws ":" ws "\"tool_call\"" ws "," ws "\"tool\"" ws ":" ws "\"calc\"" ws "," ws "\"args\"" ws ":" ws args ws "}"

final ::= "{" ws "\"type\"" ws ":" ws "\"final\"" ws "," ws "\"answer\"" ws ":" ws "\"use_rag\"" ws "}"

args ::= "{" ws "\"expression\"" ws ":" ws string ws "}"

string ::= "\"" string_char* "\""
string_char ::= [^"\\] | "\\" escape
escape ::= ["\\/bfnrt]

ws ::= [ \t\n\r]*
